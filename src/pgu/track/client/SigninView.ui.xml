<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
            xmlns:g="urn:import:com.google.gwt.user.client.ui"
            xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">

    <g:HTMLPanel>

        <b:FluidContainer>
          <b:Row>
            <b:Column size="4">
              <b:Alert type="SUCCESS" close="false">RPI Family
                <b:TextBox ui:field="familyUser" placeholder="Your name"/>
                <b:Button ui:field="familyBtn">Enter</b:Button>
              </b:Alert>
            </b:Column>
            <b:Column size="4">
              <b:Alert type="INFO" close="false">RPI Editorial
                <b:TextBox ui:field="editorialUser" placeholder="Your name"/>
                <b:Button ui:field="editorialBtn">Enter</b:Button>
              </b:Alert>
            </b:Column>
            <b:Column size="4">
              <b:Alert type="WARNING" close="false">Admin
                <b:PasswordTextBox ui:field="passBox" />
                <b:Button ui:field="adminBtn">Enter</b:Button>
              </b:Alert>
            </b:Column>
          </b:Row>
        </b:FluidContainer>
    
    </g:HTMLPanel>
</ui:UiBinder> 